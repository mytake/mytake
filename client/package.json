  "jest": {
    "testRunner": "jest-circus/runner",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/bin/",
      "<rootDir>/build/"
    ],
    "moduleNameMapper": {
      "network": "<rootDir>/test/vcr.mjs"
    },
    "setupFiles": [
      "./test/shim.js",
      "./test/setup.js"
    ],
    "verbose": true
  },
  "jest-junit": {
    "outputDirectory": "build/test-results/jest"
  }
